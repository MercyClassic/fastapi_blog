[flake8]
exclude =
    */venv/*
    *.git
    */__pycache__/*
    */__init__.py
    */db/migrations/*
    */db/base.py
    src/media
    src/logs
ignore = A003, VNE003, W503, B010
extend-immutable-calls = Depends, fastapi.Depends, fastapi.params.Depends
max-line-length=100
max-cognitive-complexity=8
inline-quotes = '
